; Mete en memoria 10 valores diferentes a partir de la direcci√≥n 256
; Se manda llamar una subrutina para guardar en memoria
		MOV		R0, #0
		MOV		R1, #10
		mov		r2, #256  ; Inicio de la memoria
again	SUBS		R1, R1, #1
		add		r2, r2, #4
		BL		guardar
		BNE		again
		END
		
guardar
		str		r1, [r2]
		mov	pc, r14 ; equivale a return
